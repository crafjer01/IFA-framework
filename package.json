{
  "name": "ifa-framework",
  "version": "0.1.9",
  "description": "IDCAS web automation framework with TypeScript and Playwright",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/global.d.ts",
  "bin": {
    "Cognito": "./bin/cognito.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./core/*": {
      "types": "./dist/core/*.d.ts",
      "import": "./dist/core/*.js"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "clean": "rm -rf dist bin",
    "copy:bin": "mkdir bin && cp src/bin/ifa.js bin/ifa.js",
    "compile": "tsc",
    "dev": "tsc --watch",
    "test": "playwright test",
    "lint": "eslint src/**/*.ts",
    "start": "node dist/cli.js init",
    "build": "npm run clean && npm run compile && npm run copy:bin",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "playwright",
    "automation",
    "testing",
    "ai",
    "typescript",
    "web-testing",
    "visual-testing",
    "self-healing"
  ],
  "author": "IDCAS Team",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "^1.55.0",
    "chalk": "^5.6.2",
    "commander": "^11.1.0",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.1",
    "glob": "^10.4.5",
    "inquirer": "^9.3.7",
    "ora": "^7.0.1",
    "playwright": "^1.40.0",
    "yaml": "^2.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "fuse.js": "^2.2.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "tsx": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "tsx": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": "https://github.com/fau-framework/fau/issues"
  },
  "homepage": ""
}
